	<ThingDef ParentName="EggFertBase">
		<defName>FungiGiantWormFive</defName>
		<label>Worm egg</label>
		<description>Which kind of worm will hatch out of this one?</description>

		<graphicData>
			<texPath>Things/Eggs/WormEgg/FungiWormEgg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>2.4</drawSize>


		</graphicData>
		<stackLimit>1</stackLimit>
		<comps>
			<li Class="Fungi.CompProperties_customHatcher">

				<thingsToHatch>

					<li>DruidWorg</li>

				</thingsToHatch>
				<hatcherDaystoHatch>0.0005</hatcherDaystoHatch>


				<scanForFaction>true</scanForFaction>
				<factionString>Alien_Fungi</factionString>

				<spawnsFilth>true</spawnsFilth>
				<filthDef>Filth_Slime</filthDef>
				<amountOfFilth>5</amountOfFilth>

				<berzerkAfterHatch>true</berzerkAfterHatch>
				<changeAge>true</changeAge>
				<ageInYears>2</ageInYears>
		



				<doExplosion>true</doExplosion>
				<explosionRadius>2</explosionRadius>
				<getsKilledInExplosion>false</getsKilledInExplosion>
				<filthOrBloodToSpawn>Filth_DruidWorm</filthOrBloodToSpawn>
				<explosionDamageDef>WorgCapsule</explosionDamageDef>
				<armorPenetrationExplosion>10</armorPenetrationExplosion>
				<damageAmountExplosion>5</damageAmountExplosion>

				<destroyPawn>false</destroyPawn>
			</li>
		</comps>

	</ThingDef>
	
