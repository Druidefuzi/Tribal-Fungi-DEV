<Defs>

	<ThingDef ParentName="EggFertBase">
		<defName>FungiGrowEgg</defName>
		<label>Worm egg</label>
		<description>Which kind of worm will hatch out of this one?</description>
		<graphicData>
			<texPath>Things/Eggs/WormEgg/FungiWormEgg</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.75</drawSize>


		</graphicData>
		<stackLimit>1</stackLimit>
		<statBases>
			<MarketValue>7.0</MarketValue>
		</statBases>
		<comps>
			<li Class="Fungi.CompProperties_customHatcher">

				<thingsToHatch>

					<li>MushroomWorm</li>


				</thingsToHatch>
				<hatcherDaystoHatch>0.0005</hatcherDaystoHatch>

				<spawnsThings>true</spawnsThings>
				<thingToSpawn>FungiGiantWormTwo</thingToSpawn>
				<amountOfThings>1</amountOfThings>


				<destroyPawn>true</destroyPawn>
			</li>
		</comps>

	</ThingDef>
