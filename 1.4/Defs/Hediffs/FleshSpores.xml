        <mentalStateGivers>
          <li>
            <mentalState>Wander_Psychotic</mentalState>
            <mtbDays>1</mtbDays>
          </li>
        </mentalStateGivers>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.3</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.3</offset>
          </li>
        </capMods>
      </li>
      <li>
        <label>Being eaten up</label>
