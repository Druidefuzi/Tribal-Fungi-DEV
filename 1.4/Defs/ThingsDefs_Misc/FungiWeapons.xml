                        <stuffCategories>
    <li>Shrooman</li>
    </stuffCategories>	
    <costList>
      <Meat_Human>30</Meat_Human>
      <RAW_SHROOMANS_RESOURCE_DEF>30</RAW_SHROOMANS_RESOURCE_DEF>
    </costList>
    <statBases>
      <WorkToMake>3500</WorkToMake>
      <Mass>0.6</Mass>
      <AccuracyTouch>0.96</AccuracyTouch>
      <AccuracyShort>0.87</AccuracyShort>
      <AccuracyMedium>0.67</AccuracyMedium>
      <AccuracyLong>0.54</AccuracyLong>
      <RangedWeapon_Cooldown>1</RangedWeapon_Cooldown>
    </statBases>
    <weaponTags>
      <li>FungiRanged</li>
      <li>SporeForce</li>
    </weaponTags>
    <recipeMaker>
      <recipeUsers>
			<li>Sporeforge</li>
      </recipeUsers>
		<skillRequirements>
			<Crafting>2</Crafting>
      </skillRequirements>
    <researchPrerequisite>AdvancedShroomwormBiology</researchPrerequisite>
    </recipeMaker>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Plaguespores</defaultProjectile>
        <warmupTime>1.4</warmupTime>
        <ticksBetweenBurstShots>8</ticksBetweenBurstShots>
        <burstShotCount>3</burstShotCount>
        <range>20</range>
        <soundCast>SporegunSound</soundCast>
      </li>
    </verbs>
    <tools>
      <li>
        <label>Blade</label>
        <capacities>
          <li>Cut</li>
          <li>Poke</li>
        </capacities>
        <power>12</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>Fleshmouth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>7</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>	 
