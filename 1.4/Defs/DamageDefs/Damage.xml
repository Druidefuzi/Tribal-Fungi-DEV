<Defs>
 

  <DamageDef ParentName="Scratch">
    <defName>FlesheatingSpores</defName>
    <label>Flesh eating spores</label>
    <externalViolence>true</externalViolence>
    <deathMessage>{0} eaten to death.</deathMessage>
	  <hediff>ChemicalBurn</hediff>
<additionalHediffs>
<li>
    <hediff>Fungi_hediff_Spores</hediff>
        <severityPerDamageDealt>0.022</severityPerDamageDealt>
        <victimSeverityScaling>ToxicResistance</victimSeverityScaling>
        <victimSeverityScalingByInvBodySize>true</victimSeverityScalingByInvBodySize>
</li>
</additionalHediffs>
    <hediffSolid>Crack</hediffSolid>
    <impactSoundType>Toxic</impactSoundType>
    <damageEffecter>Impact_Toxic</damageEffecter>
    <armorCategory>Sharp</armorCategory>
    <stabChanceOfForcedInternal>0.8</stabChanceOfForcedInternal>
    <overkillPctToDestroyPart>0.25~1.0</overkillPctToDestroyPart>
  </DamageDef>
